{{# interceptorList }}
/**
 * Interceptor Target: {{ name }} / {{ target }}
 *  Parameters: {{{ nativeParamTypes }}}
 */
const {{ name }}_Interceptor_target = ptr({{ target }})

Interceptor.attach(
  {{ name }}_Interceptor_target,
  {
    onEnter: args => {
      log.verbose(
        'SidecarAgent',
        'Interceptor.attach(%s) onEnter()',
        {{ name }}_Interceptor_target.toString(16),
      )

      send(hookPayload(
        '{{ name }}',
        {{ jsArgs }}
      ), null)

    },
  }
)
