{{# interceptorList }}
/**
 * Interceptor Target: {{ name }} / {{ hexAddress }}
 *  Parameters: {{{ nativeParamTypes }}}
 */
const {{ name }}_Interceptor_target = sidecarModuleBaseAddress.add({{ hexAddress }})

Interceptor.attach(
  {{ name }}_Interceptor_target,
  {
    onEnter: args => {
      log.verbose(
        'SidecarAgent',
        'Interceptor.attach(%s) onEnter()',
        Number({{ hexAddress }}).toString(16),
      )

      send(hookPayload(
        '{{ name }}',
        {{ jsArgs }}
      ), null)

    },
  }
)
{{/ interceptorList}}
