/**
 * Interceptor Target (Address): {{ name }} / {{ target }}
 *  Parameters: {{{ nativeParamTypes }}}
 */
const {{ name }}_Interceptor_target = sidecarModuleBaseAddress.add({{ target }})

Interceptor.attach(
  {{ name }}_Interceptor_target,
  {
    onEnter: args => {
      log.verbose(
        'SidecarAgent',
        'Interceptor.attach(0x%s) onEnter()',
        Number({{ target }}).toString(16),
      )

      send(hookPayload(
        '{{ name }}',
        {{ jsArgs }}
      ), null)

    },
  }
)
